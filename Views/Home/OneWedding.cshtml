<div class="row">
    <div class="col-6">
        <h1>@ViewBag.OneWedding.WedderOne && @ViewBag.OneWedding.WedderTwo</h1>
        <h1>Date: @ViewBag.OneWedding.Date</h1>
        <img class="card-img-top" src="@ViewBag.OneWedding.WeddingAddress" alt="Space Cat">
        

        <h3>Guests:</h3>
        <ul>
            @foreach(var g in @ViewBag.OneWedding.GuestsWhoGoing)
            {
                <li>@g.User.FirstName</li>
            }
        </ul>